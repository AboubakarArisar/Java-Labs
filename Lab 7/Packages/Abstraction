interface Animal
{
	void legs();
	void eat();
}
class cat implements Animal
{
	public void legs()
	{
		System.out.println("These are cat legs");
	}
	public void eat()
	{
		System.out.println("Cat is eating");
	}
}
class caterpiller implements Animal
{
	public void legs()
	{
		System.out.println("These are caterpiller legs");
	}
	public void eat()
	{
		System.out.println("Caterpiller is eating");
	}
}
class spider implements Animal
{
	public void legs()
	{
		System.out.println("These are spider legs");
	}
	public void eat()
	{
		System.out.println("spider is eating");
	}
}
public class Task2 {
	public static void main(String ar[])
	{
		spider s = new spider();
		s.legs();
		s.eat();
		System.out.println();

		caterpiller ct = new caterpiller();
		ct.legs();
		ct.eat();
		System.out.println();

		cat c = new cat();
		c.legs();
		c.eat();
		System.out.println();
	}

}
