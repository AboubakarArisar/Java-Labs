import java.util.Scanner;
public class Methodoverloading
{
    public static void method(int n, char c)
    {
        System.out.println("Integer is : " + n +"\nCharacter is : " + c);
    }
    public static void method(char c,int n)
    {
        System.out.println("Character is : " + c +"\nInteger is : " + n);
    }
    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        int n;
        char c;
        System.out.print("Enter the number : ");
        n = sc.nextInt();
        System.out.print("Enter the character : ");
        c = sc.next().charAt(0);
        System.out.println("Method 1 call");
        method(n,c);
        System.out.println("Method 2 call");
        method(c,n);


    }
}
