import java.util.Scanner;
class Person
{
    String name,id,address;
    void printdata()
    {
        System.out.println(name + " " + id + " "+address);
    }

}
class Student extends Person
{
    int semester;
    String department;

    void printdata()
    {

        System.out.println(name + " " + id +" "+department+" " + semester+ " "+address);
    }

}
public class inheritence
{
    public static void main(String[] args)
    {
        Student student = new Student();
        student.name = "Aboubakar";
        student.id = "107";
        student.address = "sukkur";
        student.semester = 2;
        student.department = "CS";
        student.printdata();
        
    }
}
